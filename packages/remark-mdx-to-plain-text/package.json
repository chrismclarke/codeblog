{
  "name": "remark-mdx-to-plain-text",
  "version": "3.0.0",
  "description": "Remove MDX formatting",
  "license": "MIT",
  "keywords": [
    "text",
    "markdown",
    "mdx",
    "plain",
    "text"
  ],
  "repository": "codeblog/codeblog",
  "bugs": "https://github.com/codeblog/codeblog/issues",
  "author": "Jarred Sumner <jarred@jarredsumner.com> (https://jarredsumner.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
    "Ohtake Tomohiro <ohtake.tomohiro@jp.fujitsu.com>"
  ],
  "browser": "index.js",
  "main": "index.js",
  "devDependencies": {
    "browserify": "^16.0.0",
    "nyc": "^13.0.0",
    "prettier": "^1.14.2",
    "remark": "^10.0.0",
    "remark-cli": "^6.0.0",
    "remark-mdx": "^1.0.2",
    "remark-preset-wooorm": "^4.0.0",
    "tape": "^4.4.0",
    "tinyify": "^2.4.3",
    "unist-builder": "^1.0.2",
    "xo": "^0.23.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier --write \"**/*.js\"",
    "test-api": "node test.notjest.js",
    "test": "yarn --silent format && yarn test-api"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "ignores": [
      "strip-markdown.js"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  },
  "gitHead": "c99187293b3aa1eebd7e1240c666d626a474df72"
}
